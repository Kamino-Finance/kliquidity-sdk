{
  "name": "@kamino-finance/kliquidity-sdk",
  "version": "6.2.2",
  "description": "Typescript SDK for interacting with the Kamino Liquidity (kliquidity) protocol",
  "repository": {
    "type": "git",
    "url": "git+github.com:Kamino-Finance/kliquidity-sdk.git"
  },
  "author": "Kamino Finance",
  "homepage": "https://kamino.finance",
  "bugs": {
    "url": "https://github.com/Kamino-Finance/kliquidity-sdk/issues"
  },
  "keywords": [
    "solana",
    "anchor",
    "kliquidity",
    "kamino",
    "clmm",
    "liquidity",
    "dex"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf dist/; tsc",
    "test": "npx ts-mocha tests/**/*.test.ts --exit",
    "watch": "tsc --watch",
    "lint": "yarn prettier --check .; yarn eslint .",
    "lint:fix": "yarn prettier --write .; yarn eslint . --fix",
    "codegen": "yarn codegen:kliquidity && yarn codegen:meteora && yarn codegen:raydium && yarn codegen:whirlpools",
    "codegen:kliquidity": "yarn anchor-client-gen src/kamino-client/idl.json ./src/kamino-client/ --program-id 6LtLpnUFNByNXLyCoK9wA2MykKAmQNZKBdY8s47dehDc",
    "codegen:meteora": "yarn anchor-client-gen src/meteora_client/idl.json ./src/meteora_client/ --program-id LBUZKhRxPF3XUpBCjp4YzTKgLccjZhTSDM9YuVaPwxo",
    "codegen:raydium": "yarn anchor-client-gen src/raydium_client/idl.json ./src/raydium_client/ --program-id CAMMCzo5YL8w4VFF8KVHrK22GGUsp5VTaW7grrKgrWqK",
    "codegen:whirlpools": "yarn anchor-client-gen src/whirlpools-client/idl.json ./src/whirlpools-client/ --program-id whirLbMiicVdio4qvUfM5KAg6Ct8VwpYzGff3uctyCc",
    "start-validator": "solana-test-validator $(./deps/test-validator-params.sh)",
    "start-validator-and-test": "yarn start-server-and-test 'yarn start-validator' http://127.0.0.1:8899/health test",
    "dump-programs": "./deps/dump-from-mainnet.sh"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@coral-xyz/borsh": "^0.30.1",
    "@hubbleprotocol/hubble-config": "^5.0.0",
    "@hubbleprotocol/scope-sdk": "^5.0.20",
    "@jup-ag/api": "^6.0.24",
    "@orca-so/whirlpool-client-sdk": "^0.0.8",
    "@orca-so/whirlpool-sdk": "^0.4.2",
    "@raydium-io/raydium-sdk": "=1.3.1-beta.5",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.78.4",
    "axios": "^1.2.6",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "decimal.js": "^10.3.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/bs58": "^4.0.4",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.7",
    "@types/node-fetch": "^2.6.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "anchor-client-gen": "^0.28.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.1.0",
    "chai-decimaljs": "^0.0.1",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.27.1",
    "mocha": "^10.2.0",
    "prettier": "^3.3.2",
    "start-server-and-test": "^2.0.0",
    "ts-mocha": "^10.0.0",
    "tweetnacl-ts": "^1.0.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
