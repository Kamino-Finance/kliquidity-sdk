{
  "name": "@kamino-finance/kliquidity-sdk",
  "version": "8.5.3",
  "description": "Typescript SDK for interacting with the Kamino Liquidity (kliquidity) protocol",
  "repository": {
    "type": "git",
    "url": "git+github.com:Kamino-Finance/kliquidity-sdk.git"
  },
  "author": "Kamino Finance",
  "homepage": "https://kamino.finance",
  "bugs": {
    "url": "https://github.com/Kamino-Finance/kliquidity-sdk/issues"
  },
  "keywords": [
    "solana",
    "anchor",
    "kliquidity",
    "kamino",
    "clmm",
    "liquidity",
    "dex"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/**/*.ts"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/; yarn tsc",
    "build:test": "yarn tsc --project ./tests/tsconfig.json",
    "test": "yarn build:test && yarn ts-mocha tests/**/*.test.ts --exit",
    "watch": "tsc --watch",
    "lint": "yarn prettier --check .; yarn eslint .",
    "lint:fix": "yarn prettier --write .; yarn eslint . --fix",
    "anchor-client-gen": "yarn tsx ./node_modules/anchor-client-gen/src/main.ts",
    "codegen": "rm -rf src/@codegen && yarn codegen:kliquidity && yarn codegen:meteora && yarn codegen:raydium && yarn codegen:whirlpools",
    "codegen:kliquidity": "yarn anchor-client-gen src/idl/kliquidity.json ./src/@codegen/kliquidity --program-id 6LtLpnUFNByNXLyCoK9wA2MykKAmQNZKBdY8s47dehDc",
    "codegen:meteora": "yarn anchor-client-gen src/idl/meteora.json ./src/@codegen/meteora --program-id LBUZKhRxPF3XUpBCjp4YzTKgLccjZhTSDM9YuVaPwxo",
    "codegen:raydium": "yarn anchor-client-gen src/idl/raydium.json ./src/@codegen/raydium --program-id CAMMCzo5YL8w4VFF8KVHrK22GGUsp5VTaW7grrKgrWqK",
    "codegen:whirlpools": "yarn anchor-client-gen src/idl/whirlpools.json ./src/@codegen/whirlpools --program-id whirLbMiicVdio4qvUfM5KAg6Ct8VwpYzGff3uctyCc",
    "start-validator": "solana-test-validator $(./deps/test-validator-params.sh)",
    "start-validator-and-test": "yarn start-server-and-test 'yarn start-validator' http://127.0.0.1:8899/health test",
    "dump-programs": "./deps/dump-from-mainnet.sh"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@coral-xyz/borsh": "^0.30.1",
    "@hubbleprotocol/hubble-config": "^5.0.0",
    "@jup-ag/api": "6.0.39",
    "@kamino-finance/scope-sdk": "^10.1.0",
    "@orca-so/common-sdk": "^0.6.11",
    "@orca-so/whirlpools": "^2.0.0",
    "@orca-so/whirlpools-core": "^2.0.0",
    "@raydium-io/raydium-sdk-v2": "^0.1.126-alpha",
    "@solana-program/address-lookup-table": "^0.8.0",
    "@solana-program/compute-budget": "^0.9.0",
    "@solana-program/system": "^0.8.0",
    "@solana-program/token": "^0.6.0",
    "@solana-program/token-2022": "^0.5.0",
    "@solana/compat": "^2.3.0",
    "@solana/kit": "^2.3.0",
    "@solana/sysvars": "^2.3.0",
    "axios": "^1.8.4",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "buffer-layout": "^1.2.2",
    "decimal.js": "^10.3.1",
    "fzstd": "^0.1.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/bs58": "^4.0.4",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.7",
    "@types/node-fetch": "^2.6.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "anchor-client-gen": "git+https://github.com/kklas/anchor-client-gen.git",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.1.0",
    "chai-decimaljs": "^0.0.1",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.27.1",
    "mocha": "^10.2.0",
    "prettier": "^3.3.2",
    "start-server-and-test": "^2.0.0",
    "ts-mocha": "^10.0.0",
    "tsx": "^4.19.3",
    "tweetnacl-ts": "^1.0.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
